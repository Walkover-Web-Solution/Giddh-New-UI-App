  "scripts": {
    "build:routes": "babel ./routes -d dist/public/routes --copy-files",
    "build:server": "babel ./server.js -d dist/ --copy-files",
    "build:electron": "babel ./main.js -d dist/electron/",
    "bundle:dev:electron": "npm run build:electron && webpack --config ./config/webpack.electron.config.js --env=development --progress --profile --colors --display-error-details",
    "bundle:dev": "webpack --config ./config/webpack.webapp.config.js --env=development --progress --profile --colors --display-error-details",
    "lint": "eslint ./{server,app,config}/**/*{config,''}.js",
    "start": "node server",
    "electron": "electron ./dist/electron/main.js",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "server:webapp": "npm run webpack-dev-server -- --config ./config/webpack.webapp.config.js --open --progress --profile --watch --content-base app/",
    "server:dev": "npm run build:routes && npm run build:server && concurrently --kill-others \" set NODE_ENV=development && node dist/server.js\" \"npm run server:webapp\""
  },
  "engines": {
    "node": ">= 4.2.1",
